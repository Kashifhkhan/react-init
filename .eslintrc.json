{
    "extends": ["airbnb", "eslint:recommended"],
    "env": {
        "node": false,
        "es6": true,
        "browser": true
    },
    "rules": {
        "semi": "error",
        "no-unused-vars": 1,
        "func-names": "off",
        "indent": 1,
        "no-else-return": "off",
        "prefer-arrow-callback": "off",
        "no-undef": 1,
        "no-use-before-define": "off",
        "comma-dangle": "off",
        "eol-last": "off",
        "no-trailing-spaces": 1,
        "linebreak-style": "off",
        "no-console": "off",
        "no-restricted-globals": "off",
        "object-shorthand": 1,
        "no-shadow": "off",
        "no-debugger": "off",
        "prefer-const": 1,
        "no-multiple-empty-lines": "off",
        "react/jsx-filename-extension": "off",
        "import/no-unresolved": "off"
    }
}
